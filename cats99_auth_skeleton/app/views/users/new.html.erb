<h1>Create User</h1>

<form action="<%= users_url %>" method="post">

  <label for="user_username">Username</label>
  <input
     id="user_username"
     name="user[user_name]"
     type="text">
  <br>

  <label for="user_password">Password</label>
  <input
     id="user_password"
     name="user[password]"
     type="password">
  <br>

  <input type="submit" value="<%= message %>">
</form>